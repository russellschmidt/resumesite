<h1>Users</h1>
<p>Users on this site</p>

<!-- table of site users -->
<table>
	<thead>
		<tr>
			<th>Username</th>
			<th>Email</th>
			<th>Link</th>
		<tr>
	</thead>
	<tbody>
<% @users.each do |user| %>
	<tr>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= link_to "Go", user %></td>
	</tr>
<% end %>	
	</tbody>	
</table>