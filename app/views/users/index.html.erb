<h1>Users</h1>
<p>Users on this site</p>
<p>Make a new user <%= link_to "here", new_user_path %></p>
<!-- table of site users -->
<table>
	<thead>
		<tr>
			<th>Username</th>
			<th>Email</th>
			<th>Visit</th>
			<th>Edit</th>
		<tr>
	</thead>
	<tbody>
<% @users.each do |user| %>
	<tr>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= link_to "Visit", user %></td>
		<td><%= link_to "Edit", edit_user_path(user) %></td>
	</tr>
<% end %>	
	</tbody>	
</table>
